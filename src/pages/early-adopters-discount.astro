---
import { getEntryBySlug } from "astro:content";
import TextLayout from "../layouts/TextLayout.astro";

const entry = await getEntryBySlug("static", "early-adopters-discount");
const { Content } = await entry.render();
---

<TextLayout title={entry.data.title} supportText={entry.data.supportText}>
  <Content />
</TextLayout>
